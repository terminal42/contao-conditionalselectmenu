{
    "name": "terminal42/contao-conditionalselectmenu",
    "description": "conditional select menu extension for Contao Open Source CMS",
    "keywords": ["form", "select", "conditional", "contao"],
    "type": "contao-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "terminal42 gmbh",
            "homepage": "https://www.terminal42.ch"
        }
    ],
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/terminal42"
        },
        {
            "type": "other",
            "url": "https://ko-fi.com/terminal42"
        }
    ],
    "support": {
        "issues": "https://github.com/terminal42/contao-conditionalselectmenu/issues",
        "source": "https://github.com/terminal42/contao-conditionalselectmenu"
    },
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-json": "*",
        "contao/core-bundle": "^4.9 || ^5.0",
        "doctrine/dbal": "^2.11 || ^3"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0",
        "terminal42/contao-build-tools": "dev-main"
    },
    "conflict": {
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "autoload": {
        "psr-4": {
            "Terminal42\\ConditionalSelectMenuBundle\\": "src/"
        }
    },
    "extra": {
        "contao-manager-plugin": "Terminal42\\ConditionalSelectMenuBundle\\ContaoManager\\Plugin"
    },
    "config": {
        "allow-plugins": {
            "contao-components/installer": false,
            "contao/manager-plugin": false,
            "php-http/discovery": false,
            "terminal42/contao-build-tools": true
        }
    }
}
